        );      







        /*
        * The Item Content Carousel Function
        */
        function ItemImageCarousel_ItemLoadCallback(itemImageCarousel, state) 
        {
            for (var i = itemImageCarousel.first; i <= itemImageCarousel.last; i++) 
            {
                if (itemImageCarousel.has(i)) 
                {
                    continue;
                }
                if (i > itemImageList.length) 
                {
                    break;
                }
                itemImageCarousel.add(i, ItemImageCarousel_GetItemHTML(itemImageList[i - 1]));
            }
        };

        /*
        * Item Image html creation helper.
        */
        function ItemImageCarousel_GetItemHTML(item) 
        {
            return item;
            //  return '<img src="' + item.url + '" width="75" height="75" alt="' + item.url + '" />';
        };


        /*
