    <script type="text/javascript">
        function submitRating(evt) 
        {
//            var tmp = evt.target.getAttribute('id').substr(5);
//            var widgetId = tmp.substr(0, tmp.indexOf('_'));
//            var starNbr = tmp.substr(tmp.indexOf('_') + 1);
//            
//            $.get("./SubmitRating.aspx",
//                    { ratingID: widgetId, value: starNbr },
//                    function(data) {
//                        //  alert(data);
//                        var ratingLabel = document.getElementById("ratingLabel_" + widgetId);
//                        ratingLabel.innerHTML = "<font color=gray size=1.8>Thank You</font>";
//                    }
//                 );



            var tmp = evt.target.getAttribute('id').substr(5);
            var widgetId = tmp.substr(0, tmp.indexOf('_'));
            var starNbr = tmp.substr(tmp.indexOf('_') + 1);
                        
            var doWhat = document.getElementById('rating_' + widgetId).getAttribute('doWhat');
            /*
                doWhat = 0 => Call SubmitRating.aspx
                doWhat = 1 => message = "Please Login.";
                doWhat = 2 => message = "You cannot Rate the Item you Submitted.";
                doWhat = 3 => message = "You have already Rated this Item before. Thank You.";
            */

            //  alert("doWhat: " + doWhat);

            if (doWhat != null) 
            {
                switch(doWhat)
                {
                    case ('0'):
                        $.get("./SubmitRating.aspx",
                            { ratingID: widgetId, value: starNbr },
                            function(data) {
                                //  alert(data);
                                var ratingLabel = document.getElementById("ratingLabel_" + widgetId);
                                ratingLabel.innerHTML = "<font color=gray size=1.8>Thank You</font>";
                            }
                         );
                        break;
                    case ('1'):                        
                        alert("Please Login.");
                        break;
                    case ('2'):
                        alert("You cannot Rate the Item you Submitted.");
                        break;
                    case ('3'):
                        alert("You have already Rated this Item before. Thank You.");
                        break;
                    default:
                        break;    
                      
                                             
                }
            }


           
        }

        function jqueryInit() 
        {
            init_rating();
            //  $('div.rating/img').bind('click', submitRating);
            $('span.rating/img').bind('click', submitRating);
            
        }

        function doNothing() 
        {

        }

       

        $(document).ready(jqueryInit);    
    </script>
    
