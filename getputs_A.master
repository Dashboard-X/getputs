        */
        function ItemContentCarousel_ItemLoadCallback(itemContentCarousel, state) 
        {
            for (var i = itemContentCarousel.first; i <= itemContentCarousel.last; i++) {
                if (itemContentCarousel.has(i)) {
                    continue;
                }
                if (i > itemContentList.length) {
                    break;
                }
                itemContentCarousel.add(i, ItemImageCarousel_GetItemHTML(itemContentList[i - 1]));
            }
        }

        /*
        * Item Content html creation helper.
        */
        function ItemContentCarousel_GetItemHTML(item) 
        {
            return item;
            //  return '<img src="' + item.url + '" width="75" height="75" alt="' + item.url + '" />';
        };

        


    
